class Solution {
    public int maximumWealth(int[][] accounts) {
        int wealth=0,r=accounts.length,c=accounts[0].length,temp=0;
        for(int i=0;i<r;i++){
            temp=0;
            for(int j=0;j<c;j++){
                temp+=accounts[i][j];
            }
            if(temp>wealth){
                wealth=temp;
            }
        }
    return wealth;}
}
